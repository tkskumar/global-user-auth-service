{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop":false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "target": "es6",
    "allowJs": true,
    "noEmit": true,
    "sourceMap": true,
    "outDir": "",
    "baseUrl": "./"
  },
  "exclude": ["node_modules"]
}
